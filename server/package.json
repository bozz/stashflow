    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "lodash": "^4.17.21",
    "papaparse": "^5.1.1",
    "pino": "^5.16.0",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "jest": "^25.1.0",
